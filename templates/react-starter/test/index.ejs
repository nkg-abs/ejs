<%- include('../partials/mocks') %>
import { render } from '@testing-library/react';
import <%= componentName %> from '.';

describe('<%= componentName %>', () => {
	test('renders the component appropriately', () => {
		const component = render(<%= componentName %>());
		const {
			container: { children: [rootElement] },
		} = component;

		expect(rootElement).toBeInTheDocument();
		<%_ if(childCount){ _%>
		<%_ map(content, ({ name }) => { _%>
		expect(component.getByRole('<%= name %>')).toBeInTheDocument();
		<%_ }); _%>
		<%_ } _%>
	});
});
