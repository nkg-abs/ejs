import { React } from 'react';
<%- include('imports'); _%>

const <%= componentName %> = (<%_ if(usesContext){ _%> context <%_ } _%>) =>
	<<%= type %><%- include('props'); _%>>
	<%_ if(childCount){ _%>
	<%_ map(content, ({ name }) => { _%>
		<<%= properCase(name) %> { ...context }/>
	<%_ }); _%>
	<%_ } else { _%>
		<%= content %>
	<%_ } _%>
	</<%= type %>>;

export default <%= componentName %>;
