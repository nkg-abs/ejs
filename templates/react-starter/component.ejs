import { React } from 'react';
<%_ map(imports, ({ modulePath, name }) => { _%>
import <%= name %> from '<%= modulePath %>';
<%_ }); _%>

const <%= componentName %> = (<%_ if(usesContext){ _%> context <%_ } _%>) =>
	<<%= type %> <%_ if(propCount){%> { ...{ <% map(props, (value, name) => { %><%= name %>: '<%= value %>' <% }); %>} } <%_ } _%>>
	<%_ if(childCount){ _%>
	<%_ map(content, ({ name }) => { _%>
		<<%= properCase(name) %> { ...context }/>
	<%_ }); _%>
	<%_ } else { _%>
	<%= content %>
	<%_ } _%>
	</<%= type %>>;

export default <%= componentName %>;
